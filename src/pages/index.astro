---
import Layout from '../layouts/Layout.astro';

import Header from '../components/header.astro';
import Title from '../components/title.astro';
import Navbar from '../components/navbar.astro';

const links: {name: string, href: string, button: boolean}[] = [
    {
        name: "Servicios",
        href: "#",
        button: false
    },
    {
        name: "Precios",
        href: "#",
        button: false
    },
    {
        name: "Nosotros",
        href: "#",
        button: false
    },
    {
        name: "Contacto",
        href: "#",
        button: true
    },
]
---

<Layout title="Modern Template">
    <main>
        <Header>
            <Title>Lorem Ipsum Inc.</Title>
            <Navbar links={links} />
        </Header>
    </main>
</Layout>
